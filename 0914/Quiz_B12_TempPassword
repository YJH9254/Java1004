package quiz;

public class B12_TempPassword {
	/*
	 	랜덤 알파벳 대문자로 구성된 랜덤 4자리 비밀번호를
	 	생성하여 출력하는 프로그램을 만들어보세요
	 	
	 	-> 65 ~ 90의 숫자를 4개 만들어보세요
	*/
	public static void main(String[] args) {
		
		//65~90 대문자 알파벳
		for (int i = 0; i < 4; i++) {
			char ch = (char)(Math.random() * 26 + 65);
			
			System.out.print(ch);
		} 
		System.out.println();
	}
}
